# Main Database (PostgreSQL/MySQL/SQLite - choose one)
DATABASE_URL="postgresql://user:password@localhost:5432/tap_main?schema=public"

# Meiko Database (MySQL)
DATABASE_MEIKO_URL="mysql://user:password@external-host:3306/meiko"

# TAP Database (SQL Server)
DATABASE_TAP_URL="sqlserver://localhost:1433;database=tap;user=sa;password=YourPassword123;encrypt=true;trustServerCertificate=true"

# Application Settings
